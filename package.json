{
  "name": "discord-agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@epic-web/config": "^1.18.3",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@prisma/client": "^6.6.0",
    "@types/express": "^5.0.1",
    "ai": "^4.3.4",
    "chrono-node": "^2.8.0",
    "cron-parser": "^5.1.1",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "openai": "^4.93.0",
    "prisma": "^6.6.0",
    "ulid": "^3.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yaml": "^2.7.1",
    "zod": "^3.24.2"
  },
  "prettier": "@epic-web/config/prettier"
}
