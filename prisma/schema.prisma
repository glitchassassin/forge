generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Queue {
  id           String   @id @default(uuid())
  secondaryKey String   @map("secondary_key")
  item         String
  createdAt    DateTime @default(now()) @map("created_at")

  @@map("queues")
}

model Agent {
  id           String   @id @default(uuid())
  secondaryKey String   @map("secondary_key")
  item         String
  createdAt    DateTime @default(now()) @map("created_at")

  @@map("agents")
}
